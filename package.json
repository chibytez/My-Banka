  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-async-to-generator": "^7.4.0",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "@sendgrid/mail": "^6.3.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "jol": "^0.1.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.1.4",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.10",
    "pg": "^7.10.0",
    "validator": "^10.11.0",
    "validatorjs": "^3.15.1",
