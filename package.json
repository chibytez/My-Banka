{
  "name": "my-banka",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test  nyc mocha ./server/test/*.spec.js --require @babel/register --exit",
    "start:dev": " nodemon  app.js --exec babel-node",
    "start": "baapp.js",
    "createTable": " node -r @babel/register server/migrations/createTable.js",
    "dropTable": " node -r @babel/register server/migrations/dropTable.js",
    "userSeed": " node -r @babel/register server/migrations/userSeed.js",
    "adminSeed": " node -r @babel/register server/migrations/adminSeed.js",
    "migration": "npm run dropTable && npm run createTable",
    "mig-test": "cross-env NODE_ENV=test babel-node server/migrations/createTable.js server/migrations/dropTable.js  ",
    "seeder": "npm run userSeed && npm run adminSeed",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov  | coveralls "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chibytez/My-Banka.git"
  },
